{
  "expo": {
    "name": "app-proteccionlaboral",
    "slug": "app-proteccionlaboral",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación necesita acceder a tu galería para seleccionar imágenes.",
          "cameraPermission": "La aplicación necesita acceder a la cámara para tomar fotos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
